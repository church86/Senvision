import java.io.File;

public class DeleteDuplicates {
	public static void deleteDuplicates(String TweetsFolder){
File[] TweetFolderArray = new File(TweetsFolder).listFiles();
for(File compFolder: TweetFolderArray){
	System.out.println(compFolder.getName());
}

	}
	
	
	
	
	private static String removeUrl(String commentstr)
    {
		
		return commentstr.replaceAll("https?://\\S+\\s?", "");
    }
	
	public static void main(String[] args){
		deleteDuplicates("../../AllTweets/Tweets");
	}
	
}
